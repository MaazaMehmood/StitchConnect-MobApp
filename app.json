{
  "expo": {
    "name": "stitch-connect",
    "slug": "stitch-connect",
    "version": "1.0.0",
    "entryPoint": "./App.js",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/SC.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "start": "expo start",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SC.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/SC.png"
    }
  }
}
